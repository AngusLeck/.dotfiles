#!/bin/bash

echo "Sending playground..."

rsync -azv --delete -e ssh $HOME/development/playground/ michael@code-assistant.com:/home/michael/development/playground

echo "Sending projects..."

rsync -azv --delete -e ssh $HOME/development/projects/ michael@code-assistant.com:/home/michael/development/projects

echo "Sending vm..."

rsync -azv --delete -e ssh $HOME/development/vm/ michael@code-assistant.com:/home/michael/development/vm

echo "Sending opensource..."

rsync -azv --delete -e ssh $HOME/development/opensource/ michael@code-assistant.com:/home/michael/development/opensource
