#!/bin/bash

echo "Sending playground..."

rsync -azv --delete -e ssh $HOME/development/playground/ michael@tyland.us:/home/michael/development/playground

echo "Sending projects..."

rsync -azv --delete -e ssh $HOME/development/projects/ michael@tyland.us:/home/michael/development/projects

echo "Sending vm..."

rsync -azv --delete -e ssh $HOME/development/vm/ michael@tyland.us:/home/michael/development/vm

